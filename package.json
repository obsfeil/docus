{
  "name": "@nuxt-themes/docus",
  "version": "1.14.3",
  "type": "module",
  "main": "./nuxt.config.ts",
  "packageManager": "pnpm@8.15.8",
  "files": [
    "app",
    "assets",
    "components",
    "composables",
    "layouts",
    "plugins",
    "server",
    "nuxt.schema.ts",
    "app.vue",
    "app.config.ts",
    "nuxt.config.ts",
    "tokens.config.ts"
  ],
  "scripts": {
    "prepare": "nuxi prepare && nuxi prepare .starters/default",
    "dev": "nuxi dev .starters/default",
    "build": "nuxi build .starters/default",
    "generate": "nuxi generate .starters/default",
    "preview": "nuxi preview .starters/default",
    "lint": "eslint .",
    "release": "release-it"
  },
  "dependencies": {
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@nuxt-themes/typography": "^0.11.0",
    "@nuxthq/studio": "^0.14.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/plausible": "^0.2.4",
    "@rollup/plugin-inject": "^5.0.5",
    "@vueuse/integrations": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "focus-trap": "^7.5.4",
    "fuse.js": "^6.6.2",
    "google-fonts-helper": "^3.6.0",
    "mkdirp": "^3.0.1"
  },
  "devDependencies": {
    "@algolia/client-search": "^4.23.3",
    "@docsearch/css": "^3.6.0",
    "@docsearch/js": "^3.6.0",
    "@nuxt-themes/elements": "^0.9.5",
    "@nuxt-themes/tokens": "^1.9.1",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools": "npm:@nuxt/devtools-edge@latest",
    "@nuxt/eslint-config": "^0.3.10",
    "@nuxt/image": "1.7.0",
    "@nuxt/kit": "npm:@nuxt/kit-edge@latest",
    "@nuxtjs/algolia": "^1.10.2",
    "@types/node": "^20.12.8",
    "eslint": "^8.57.0",
    "magicast": "^0.3.4",
    "nuxt": "^3.11.2",
    "release-it": "^16.3.0",
    "typescript": "^5.4.5",
    "vite-plugin-inspect": "^0.8.4",
    "vue": "^3.4.26"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "*"
      },
      "ignoreMissing": [
        "postcss",
        "postcss-*",
        "webpack",
        "vite"
      ]
    }
  },
  "license": "MIT",
  "release-it": {
    "npm": {
      "access": "public"
    },
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
